<%-include('../cabecalho')-%>
<h1>Edita Produto</h1>
<form method="post" enctype="multipart/form-data">
    Titulo: <input type="text" name="titulo" id="titulo" value="<%=Produto.titulo%>"><br />
    Genero: <input type="text" name="genero" id="genero" value="<%=Produto.genero%>"><br />
    Diretor: <input type="text" name="diretor" id="diretor" value="<%=Produto.diretor%>"><br />
    Foto: <input type="file" name="foto" id="foto"><br />
    Ano: <input type="number" name="ano" id="ano" value="<%=Produto.ano%>"><br />
    tempo: <input type="time" name="tempo" id="tempo" value="<%=Produto.tempo%>"><br />
    <input type="submit" value="Adicionar">
</form>
<%-include('../rodape')-%>